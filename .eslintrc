{
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:vue/essential"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-unused-vars": "error",
    "no-script-url": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle":0,
    "prefer-destructuring" :0,
    "object-shorthand" :0,
    "arrow-body-style" : 0,
    "no-restricted-syntax" : 0,
    "linebreak-style": 0,
    "vue/multi-word-component-names": 0,
    "import/no-unresolved":0,
    "spaced-comment":0,
    "import/order":0,
    "no-var":0,
    "vars-on-top":0,
    "no-plusplus":0,
    "prefer-const":0
  }
}